<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:padding="20dp"
    tools:context=".MainDashboard">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/row1"
        android:layout_width="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="10dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_height="0dp">
        <TextView
            android:id="@+id/profile"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:text="Profile"
            android:elevation="5dp"
            android:padding="10dp"
            android:background="@color/primaryColor"
            android:textColor="@color/white"
            android:layout_margin="10dp"
            android:textAlignment="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/Booking"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:id="@+id/Booking"
            android:layout_width="0dp"
            android:layout_height="50dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/profile"
            android:layout_margin="10dp"
            app:layout_constraintEnd_toStartOf="@id/History"
            android:background="@color/primaryColor"
            android:textColor="@color/white"
            android:text="Booking"
            android:padding="10dp"
            android:textAlignment="center"
            android:elevation="5dp"
            />
        <TextView
            android:id="@+id/History"
            android:layout_width="0dp"
            android:layout_height="50dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/Booking"
            android:background="@color/primaryColor"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_margin="10dp"
            android:text="History"
            android:padding="10dp"
            android:textAlignment="center"
            android:elevation="5dp"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/row2"
        android:layout_width="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/white"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@id/row1"
        android:layout_height="0dp">
        <TextView
            android:id="@+id/help"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:text="Help"
            android:elevation="5dp"
            android:padding="10dp"
            android:background="@color/primaryColor"
            android:textColor="@color/white"
            android:layout_margin="10dp"
            android:textAlignment="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/map"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:id="@+id/map"
            android:layout_width="0dp"
            android:layout_height="50dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/help"
            android:background="@color/primaryColor"
            android:textColor="@color/white"
            android:layout_margin="10dp"
            app:layout_constraintEnd_toStartOf="@id/emergency"
            android:text="Map"
            android:padding="10dp"
            android:textAlignment="center"
            android:elevation="5dp"
            />
        <TextView
            android:id="@+id/emergency"
            android:layout_width="0dp"
            android:layout_height="50dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/map"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="@color/primaryColor"
            android:textColor="@color/white"
            android:layout_margin="10dp"
            android:text="Emergency"
            android:padding="10dp"
            android:textAlignment="center"
            android:elevation="5dp"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/row2"
        android:layout_width="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/white"
        android:padding="10dp"
        app:layout_constraintTop_toBottomOf="@id/row1"
        android:layout_height="0dp">
        <TextView
            android:id="@+id/help"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:text="Help"
            android:elevation="5dp"
            android:padding="10dp"
            android:background="@color/primaryColor"
            android:textColor="@color/white"
            android:layout_margin="10dp"
            android:textAlignment="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@id/map"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:id="@+id/map"
            android:layout_width="0dp"
            android:layout_height="50dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/help"
            android:background="@color/primaryColor"
            android:textColor="@color/white"
            android:layout_margin="10dp"
            app:layout_constraintEnd_toStartOf="@id/emergency"
            android:text="Map"
            android:padding="10dp"
            android:textAlignment="center"
            android:elevation="5dp"
            />
        <TextView
            android:id="@+id/emergency"
            android:layout_width="0dp"
            android:layout_height="50dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/map"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="@color/primaryColor"
            android:textColor="@color/white"
            android:layout_margin="10dp"
            android:text="Emergency"
            android:padding="10dp"
            android:textAlignment="center"
            android:elevation="5dp"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>